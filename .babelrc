{
    "presets": [
        [
            "@babel/preset-env", 
            {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 5 versions",
                        "ie >= 9"
                    ]
                },
                "useBuiltIns": "usage"
            }
        ],
        "@babel/preset-react"
       ],
       "retainLines": true,
       "sourceMaps": "inline",
       "plugins": [
           ["import", { "libraryName": "antd", "libraryDirectory": "lib", "style": "css"}, "ant"]
       ]
}