{
	"name": "react-webpack",
	"version": "1.0.0",
	"description": "A react project with webpack",
	"main": "index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.conf.js --open",
		"build": "cross-env NODE_ENV=production webpack --progress --colors --config ./build/webpack.prod.conf.js",
		"build:perf": "cross-env NODE_ENV=production webpack --progress --colors --config ./build/webpack.perf.conf.js",
		"lint-staged": "lint-staged",
		"lint-staged:js": "eslint --ext .js,.jsx",
		"prettier": "prettier -c --write \"**/*\"",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"lint-staged": {
		"**/*.{js,jsx,tsx,ts,less,md,json}": [
			"prettier --write",
			"git add"
		],
		"**/*.{js,jsx}": "npm run lint-staged:js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint-staged"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Victor-H5/react-webpack.git"
	},
	"keywords": [
		"react",
		"webpack"
	],
	"author": "Victor",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Victor-H5/react-webpack/issues"
	},
	"homepage": "https://github.com/Victor-H5/react-webpack#readme",
	"devDependencies": {
		"@babel/core": "^7.1.6",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-decorators": "^7.7.4",
		"@babel/plugin-syntax-dynamic-import": "^7.7.4",
		"@babel/plugin-transform-runtime": "^7.1.0",
		"@babel/preset-env": "^7.1.6",
		"@babel/preset-react": "^7.0.0",
		"@babel/runtime": "^7.1.5",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.4",
		"babel-plugin-import": "^1.11.0",
		"clean-webpack-plugin": "^1.0.0",
		"cross-env": "^6.0.3",
		"css-loader": "^1.0.1",
		"eslint": "^6.7.2",
		"eslint-config-prettier": "^6.7.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-plugin-import": "^2.19.1",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-standard": "^4.0.1",
		"file-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^3.1.0",
		"less": "^3.8.1",
		"less-loader": "^4.1.0",
		"lint-staged": "^9.5.0",
		"mini-css-extract-plugin": "^0.5.0",
		"mockjs": "^1.1.0",
		"node-sass": "^4.10.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-flexbugs-fixes": "^4.1.0",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.6.0",
		"prettier": "1.19.1",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"url-loader": "^1.1.2",
		"webpack": "^4.25.1",
		"webpack-bundle-analyzer": "^3.6.0",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.10",
		"webpack-merge": "^4.2.2"
	},
	"dependencies": {
		"@babel/polyfill": "^7.0.0",
		"antd": "^3.26.0",
		"axios": "^0.18.0",
		"classnames": "^2.2.6",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"prop-types": "^15.7.2",
		"react": "^16.8.2",
		"react-dom": "^16.8.2",
		"react-hot-loader": "^4.12.18",
		"react-loadable": "^5.5.0",
		"react-redux": "^6.0.1",
		"react-router-dom": "^4.3.1",
		"redux": "^4.0.1",
		"redux-saga": "^1.0.1",
		"setprototypeof": "^1.2.0"
	}
}
